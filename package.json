{
  "name": "my-node-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for web APIs using Node",
  "scripts": {
    "start": "node cluster.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon src/index.js",
    "lint": "eslint {src,test,config}/**/*.js",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "console": "cross-env NODE_PATH=. node src/console/index.js",
    "heroku-postbuild": "NODE_ENV=production NODE_PATH=. sequelize db:migrate --url=$DATABASE_URL",
    "pm2": "pm2",
    "cleanup": "node scripts/cleanup.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "del": "^6.0.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.26.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "joi": "^17.4.0",
    "log4js": "^6.3.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pm2": "^4.5.6",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "structure": "^2.0.1",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-change": "^2.1.2",
    "chance": "^1.1.7",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^5.0.4",
    "listr": "^0.14.3",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "replace-in-file": "^6.2.0",
    "supertest": "^6.1.3",
    "supertest-as-promised": "^4.0.2"
